      <a><%= image_tag "f忍者猫1.png", class:"ninjas_a a_4", id:"ninja_a_4"%></a>
      <a><%= image_tag "f忍者8.png", class:"ninjas_a a_5", id:"ninja_a_5"%></a>
      <a><%= image_tag "f忍者8.png", class:"ninjas_a a_6", id:"ninja_a_6"%></a>
      <a><%= image_tag "f忍者7.png", class:"ninjas_a a_7", id:"ninja_a_7"%></a>
      <a><%= image_tag "f忍者8.png", class:"ninjas_a a_8", id:"ninja_a_8"%></a>
    </div>
    <div id="ninjas_b">
      <a><%= image_tag "f忍者6黄色2.png", class:"ninjas_b b_1", id:"ninja_b_1"%></a>
      <a><%= image_tag "f忍者1.png", class:"ninjas_b b_2", id:"ninja_b_2"%></a>
      <a><%= image_tag "f忍者1.png", class:"ninjas_b b_3", id:"ninja_b_3"%></a>
      <a><%= image_tag "f忍者6黄色2.png", class:"ninjas_b b_4", id:"ninja_b_4"%></a>
